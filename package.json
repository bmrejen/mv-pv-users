  "scripts": {
    "build": "./node_modules/.bin/gulp build",
    "build:aot": "rm -rf ./src/aot/ && ./node_modules/.bin/ngc -p tsconfig-aot.json && ./node_modules/.bin/rollup -c rollup-config.js",
    "dev": "yarn run hook:install && chmod +x ./bin/*.sh && ./node_modules/.bin/gulp dev",
    "prod": "./node_modules/.bin/gulp dist",
    "hook:install": "yarn run hook:post-checkout && yarn run hook:pre-commit",
    "hook:post-checkout": "cp post-checkout.dist .git/hooks/post-checkout && chmod +x .git/hooks/post-checkout",
    "hook:pre-commit": "cp pre-commit.dist .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "htmllint": "./node_modules/.bin/htmlhint src/**/*.html",
    "tslint": "./node_modules/.bin/tslint -p . -c tslint.json -t stylish",
    "sasslint": "./node_modules/.bin/sass-lint src/scss/**/*.scss -f checkstyle -v -q &> build/sasslint.xml",
    "publish:prerelease": "./node_modules/.bin/gulp publish:prerelease",
    "publish:beta": "./node_modules/.bin/gulp publish:beta",
    "publish:rc": "./node_modules/.bin/gulp publish:rc",
    "publish:prepatch": "./node_modules/.bin/gulp publish:prepatch",
    "publish:preminor": "./node_modules/.bin/gulp publish:preminor",
    "publish:patch": "./node_modules/.bin/gulp publish:patch",
    "publish:minor": "./node_modules/.bin/gulp publish:minor",
    "publish:major": "./node_modules/.bin/gulp publish:major",
    "test:unit": "./node_modules/.bin/jest",
    "test:unit:ci": "yarn run test:unit:coverage --testResultsProcessor='./node_modules/jest-jenkins-reporter'",
    "test:unit:coverage": "./node_modules/.bin/jest --runInBand --coverage --collectCoverageFrom=src/**/*.ts --collectCoverageFrom=!src/main.ts --collectCoverageFrom=!src/app/rxjs-extensions.ts --collectCoverageFrom=!src/app/routes.ts --collectCoverageFrom=!**/index.ts --collectCoverageFrom=!src/aot/**/*.ts",
    "version": "./bin/version.sh $npm_package_version"
  },
  "engines": {
    "yarn": ">= 1.9.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests/unit"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/unit/setup-jest.ts",
    "testResultsProcessor": "jest-jenkins-reporter",
    "testURL": "http://localhost",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tests/unit/tsconfig.spec.json"
      },
      "__TRANSFORM_HTML__": true
    }
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "unit.xml",
    "indent": 4
  }
}
